CREATE TABLE  MANAGERS (
    Manager_id NUMBER PRIMARY KEY,
    Name VARCHAR2(50),
    DEPT_Name VARCHAR2(50),
    Qualification VARCHAR2(100),
    Experience NUMBER
);
